{
  "name": "vppjs",
  "version": "0.1.15",
  "description": "VSOA app framework like Express.js",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "clean": "tsc --build --clean ",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "tape -r ts-node/register test/**/*.test.ts | tap-spec",
    "edgeros": "sed -i '' -e 's/\"name\": \"vppjs\"/\"name\": \"@edgeros\\/vppjs\"/g' -e '/\"dependencies\": {/,+2d' package.json",
    "prepublish": "npm run lint && npm run test && npm run build",
    "postpublish": "git checkout package.json"
  },
  "keywords": [
    "vsoa"
  ],
  "author": "clarkttfu@gmail.com",
  "license": "MIT",
  "dependencies": {
    "vsoa": "^0.2.7-c"
  },
  "devDependencies": {
    "@types/node": "^17.0.29",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.5.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
